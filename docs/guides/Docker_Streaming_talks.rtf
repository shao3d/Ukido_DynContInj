\
          let responseText = '';\
\
          eventSource.onmessage = (event) => \{\
              const data = JSON.parse(event.data);\
\
              if (data.type === 'status') \{\
                  // \uc0\u1055 \u1086 \u1082 \u1072 \u1079 \u1099 \u1074 \u1072 \u1077 \u1084  \u1089 \u1090 \u1072 \u1090 \u1091 \u1089 \
